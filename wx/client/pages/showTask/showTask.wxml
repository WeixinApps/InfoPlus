<view>
    <view class="taskCard {{control.display?'':'hide'}}">
      <view class="titleSpan">任务预览</view>
      <view class="title">{{task.title}}</view>
      <view class="detail">{{task.detail}}</view>
      <view class="info">
        由 <text class="createdBy">{{task.createdBy}}</text> 创建于 <text class="createdDate">{{task.createdDate}}</text>
      </view>
      <view class="detail">{{task.detail}}</view>
      <view class="endTime {{task.endTime?'':'hide'}}">{{task.endTime}}</view>
      <open-data type="groupName" open-gid="{{task.groupId}}"></open-data>
      <!-- <view class="createTime"></view> -->
     
      

    </view>
</view>